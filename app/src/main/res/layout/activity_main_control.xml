<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <Button
        android:id="@+id/startRequestButton"
        android:layout_width="108dp"
        android:layout_height="42dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/button1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ToggleButton
        android:id="@+id/toggleButton"
        android:layout_width="85dp"
        android:layout_height="42dp"
        app:layout_constraintHorizontal_bias="0.802"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteX="265dp"
        tools:layout_conversion_absoluteY="6dp"
        tools:layout_conversion_absoluteWidth="85dp"
        tools:layout_conversion_absoluteHeight="42dp" />

    <TextView
        android:id="@+id/printView"
        android:layout_width="365dp"
        android:layout_height="162dp"
        android:text="@string/printView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.181"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_conversion_absoluteX="9dp"
        tools:layout_conversion_absoluteY="190dp"
        tools:layout_conversion_absoluteWidth="367dp"
        tools:layout_conversion_absoluteHeight="305dp"
        android:layout_marginLeft="8dp"
        android:layout_marginBottom="25dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintVertical_bias="0.18" />

    <ImageView
        android:id="@+id/imageStator"
        android:layout_width="388dp"
        android:layout_height="259dp"
        android:layout_marginBottom="-13dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/staticwama" />

    <com.twmiwi.com.washingcontrol.ProgramSwitch
        android:id="@+id/imageRotor"
        android:layout_width="394dp"
        android:layout_height="198dp"
        android:layout_marginBottom="30dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/schalter" />


</android.support.constraint.ConstraintLayout>
